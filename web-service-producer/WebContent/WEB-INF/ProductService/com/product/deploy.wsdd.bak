<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProductService WSDL service -->

  <service name="Product" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://product.com"/>
      <parameter name="wsdlServiceElement" value="ProductService"/>
      <parameter name="schemaQualified" value="http://product.com"/>
      <parameter name="wsdlServicePort" value="Product"/>
      <parameter name="className" value="com.product.ProductSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Product"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getProducts" qname="operNS:getProducts" xmlns:operNS="http://product.com" returnQName="retNS:getProductsReturn" xmlns:retNS="http://product.com" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://product.com" returnItemQName="tns:item" xmlns:tns="http://product.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getProducts"/>

      <typeMapping
        xmlns:ns="http://product.com"
        qname="ns:>getProducts"
        type="java:com.product.GetProducts"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://product.com"
        qname="ns:>getProductsResponse"
        type="java:com.product.GetProductsResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://product.com"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
